@{
    Layout = null;
    ViewBag.pathParent = Url.Content("~").Substring(0, Url.Content("~").Length - 1);
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>AMKO</title>

    <!-- Custom CSS -->
    <link href="~/Content/custom.css" rel="stylesheet">

    <!-- Bootstrap Core CSS -->
    <link href="~/Content/bootstrap.min.css" rel="stylesheet">

    <!-- MetisMenu CSS -->
    <link href="~/Content/template/css/metisMenu.min.css" rel="stylesheet">

    <!-- Timeline CSS -->
    <link href="~/Content/template/css/timeline.css" rel="stylesheet">

    <!-- Custom CSS -->
    <link href="~/Content/template/css/sb-admin-2.css" rel="stylesheet">

    <link href="~/Content/Kendo/kendo.common.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/Kendo/kendo.blueopal.min.css" rel="stylesheet" type="text/css" />

    <!-- Custom Fonts -->
    @*<link href="~/Content/template/css/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">*@

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- jQuery -->

    <script src="~/Content/template/js/jquery.min.js"></script>
    @*<script src="~/Scripts/Kendo/kendo.web.min.js"></script>*@
    <script src="~/Scripts/Kendo/kendo.all.min.js"></script>

</head>

<body>
    <div id="wrapper">
        <!-- Navigation -->
        <nav class="navbar navbar-custom navbar-fixed-top" role="navigation" style="margin-bottom: 0">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="@ViewBag.pathParent/Home/Index">AMKO - Asset Management KPP ASTO</a>
            </div>
            <!-- /.navbar-header -->

            @*@Html.Partial("_LoginMenu")*@
        </nav>

        <div id="page-wrapper">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12">
                        <h3 class="page-header">@ViewBag.dashboard</h3>
                    </div>
                    <!-- /.col-lg-12 -->
                </div>
                <!-- /.row -->
                <div class="row">
                    <div class="col-lg-12">
                        @RenderBody()
                    </div>
                </div>
            </div>
        </div>
        <!-- /#page-wrapper -->

        <footer class="footer">
            <p class="text-footer text-right">&copy; 2017 Kalimantan Prima Persada</p>
        </footer>
    </div>
    <!-- /#wrapper -->
    <!-- Bootstrap Core JavaScript -->
    <script src="~/Content/template/js/bootstrap.min.js"></script>

    <!-- Metis Menu Plugin JavaScript -->
    <script src="~/Content/template/js/metisMenu.min.js"></script>

    <!-- Custom Theme JavaScript -->
    <script src="~/Content/template/js/sb-admin-2.js"></script>

    @RenderSection("scripts", required: false)
</body>

</html>

